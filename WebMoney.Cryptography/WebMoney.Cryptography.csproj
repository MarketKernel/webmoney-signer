<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>key.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
    <Version>4.0.0</Version>
    <Description>See: https://wiki.webmoney.ru/projects/webmoney/wiki/WMSignerFX</Description>
    <Company>WMTransfer Ltd.</Company>
    <Authors>Dmitry Kukushkin</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>Â© Dmitry Kukushkin &lt;support@wmsigner.com&gt; 2007-2021</Copyright>
    <PackageLicenseExpression>(MIT)</PackageLicenseExpression>
    <PackageProjectUrl>http://www.wmsigner.com/</PackageProjectUrl>
    <PackageTags>WebMoney,WMSigner, WMSignerFX,WMTransfer</PackageTags>
    <PackageReleaseNotes>Compatible with .Net Core</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/MarketKernel/webmoney-signer</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Encoding.CodePages" Version="6.0.0" />
  </ItemGroup>

</Project>
